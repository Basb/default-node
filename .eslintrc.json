{
    "extends": "eslint:recommended",
    "env": {
        "node": true,
        "es6": true
    },
    "rules": {
        "accessor-pairs": 2,
        "array-bracket-spacing": 2,
        "array-callback-return": 2,
        "arrow-parens": [2, "as-needed"],
        "arrow-spacing": 2,
        "block-scoped-var": 2,
        "block-spacing": 2,
        "brace-style": [2, "1tbs", { "allowSingleLine": true }],
        "camelcase": 2,
        "comma-dangle": 2,
        "comma-spacing": 2,
        "comma-style": 2,
        "complexity": 2,
        "computed-property-spacing": 2,
        "consistent-return": 2,
        "curly": 2,
        "dot-location": [2, "property"],
        "dot-notation": 2,
        "eol-last": 2,
        "eqeqeq": [2, "always", {"null": "ignore"}],
        "func-call-spacing": 2,
        "func-style": 2,
        "guard-for-in": 2,
        "indent": [2, 4, {"SwitchCase": 1}],
        "jsx-quotes": 2,
        "key-spacing": 2,
        "keyword-spacing": 2,
        "line-comment-position": 2,
        "lines-around-comment": [2, { "beforeLineComment": false, "allowBlockStart": true }],
        "max-depth": 2,
        "max-nested-callbacks": [2, 3],
        "max-params": 2,
        "new-cap": 2,
        "new-parens": [2, { "nestedBinaryExpressions": false }],
        "newline-per-chained-call": [2, { "ignoreChainWithDepth": 3 }],
        "no-alert": 2,
        "no-array-constructor": 2,
        "no-bitwise": 2,
        "no-caller": 2,
        "no-continue": 2,
        "no-div-regex": 2,
        "no-duplicate-imports": 2,
        "no-else-return": 2,
        "no-empty-function": 2,
        "no-eq-null": 2,
        "no-eval": 2,
        "no-extend-native": 2,
        "no-extra-bind": 2,
        "no-extra-label": 2,
        "no-extra-parens": 2,
        "no-implied-eval": 2,
        "no-invalid-this": 2,
        "no-iterator": 2,
        "no-labels": 2,
        "no-lone-blocks": 2,
        "no-lonely-if": 2,
        "no-loop-func": 2,
        "no-mixed-operators": 2,
        "no-multi-spaces": 2,
        "no-multiple-empty-lines": 2,
        "no-nested-ternary": 2,
        "no-new-func": 2,
        "no-new-object": 2,
        "no-new-wrappers": 2,
        "no-new": 2,
        "no-octal-escape": 2,
        "no-proto": 2,
        "no-prototype-builtins": 2,
        "no-return-assign": 2,
        "no-script-url": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-shadow": 2,
        "no-template-curly-in-string": 2,
        "no-trailing-spaces": [2, { "skipBlankLines": true }], 
        "no-undef-init": 2, 
        "no-undefined": 2,
        "no-unmodified-loop-condition": 2,
        "no-unneeded-ternary": 2,
        "no-use-before-define": 2,
        "no-useless-call": 2,
        "no-useless-computed-key": 2,
        "no-useless-concat": 2,     
        "no-useless-constructor": 2,
        "no-useless-rename": 2,
        "no-var": 2, 
        "no-void": 2,
        "no-whitespace-before-property": 2, 
        "no-with": 2,
        "object-curly-spacing": 2, 
        "object-property-newline": [2, { "allowMultiplePropertiesPerLine": true }],
        "object-shorthand": [2, "consistent"], 
        "one-var-declaration-per-line": 2,
        "one-var": [2, "never"],
        "operator-linebreak": [2, "none"],
        "padded-blocks": 2,
        "prefer-arrow-callback": 2, 
        "prefer-template": 2, 
        "quote-props": [2, "consistent-as-needed"], 
        "quotes": [2, "single"],
        "radix": 2,
        "semi-spacing": 2, 
        "semi": 2,
        "space-before-blocks": 2, 
        "space-before-function-paren": [2, "never"], 
        "space-in-parens": [2, "never"], 
        "space-unary-ops": [2, { "words": true, "nonwords": false }], 
        "spaced-comment": [2, "always"], 
        "template-curly-spacing": 2, 
        "vars-on-top": 1,
        "yoda": 2
    }
}